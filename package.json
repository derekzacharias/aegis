{
  "name": "compliance-platform",
  "version": "0.0.1",
  "private": true,
  "description": "Unified cybersecurity compliance platform covering NIST 800-53, NIST CSF, CIS, PCI DSS, and FedRAMP-focused workflows.",
  "scripts": {
    "nx": "nx",
    "build": "nx run-many --target build --all",
    "lint": "nx run-many --target lint --all",
    "test": "nx run-many --target test --all",
    "test:dom": "nx run-many --target test --projects web",
    "test:node": "nx run-many --target test --projects api,worker,shared",
    "affected:test": "nx affected --target test",
    "affected:lint": "nx affected --target lint",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "dev": "npm run dev:web",
    "dev:web": "vite --config apps/web/vite.config.ts",
    "dev:api": "ts-node --require tsconfig-paths/register --transpile-only --compiler-options '{\"module\":\"commonjs\"}' --project apps/api/tsconfig.app.json apps/api/src/main.ts",
    "dev:worker": "nx serve worker",
    "dev:nx": "nx run-many --target serve --parallel --projects web,api",
    "serve:web": "nx serve web",
    "serve:api": "nx serve api",
    "test:e2e": "npx playwright test",
    "prisma:generate": "npx prisma generate --schema apps/api/prisma/schema.prisma",
    "db:migrate": "npx prisma migrate deploy --schema apps/api/prisma/schema.prisma",
    "prisma:seed": "npx ts-node --project apps/api/prisma/tsconfig.seed.json apps/api/prisma/seed.ts",
    "test:db-smoke": "node tools/testing/prisma-smoke.cjs"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.3",
    "@prisma/client": "^5.9.1",
    "@tanstack/react-query": "^5.32.1",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "dayjs": "^1.11.10",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "highlight.js": "^11.11.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.552.0",
    "next": "14.1.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "puppeteer": "^22.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.22.3",
    "reflect-metadata": "^0.1.13",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "rxjs": "^7.8.1",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.3.3",
    "@nx/eslint": "17.3.2",
    "@nx/eslint-plugin": "^17.3.2",
    "@nx/jest": "17.3.2",
    "@nx/js": "17.3.2",
    "@nx/nest": "17.3.2",
    "@nx/node": "17.3.2",
    "@nx/react": "17.3.2",
    "@nx/vite": "17.3.2",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.21",
    "@types/react-router-dom": "^5.3.3",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "nx": "17.3.2",
    "prettier": "^3.2.5",
    "prisma": "^5.9.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
  }
}
